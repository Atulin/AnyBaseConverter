        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PackageId>Atulin.AnyBaseConverter</PackageId>
        <Version>1.0.0</Version>
        <Authors>Atulin</Authors>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>AnyBaseConverter</Title>
        <PackageVersion>1.0.1</PackageVersion>
        <PackageProjectUrl>https://github.com/Atulin/AnyBaseConverter</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Atulin/AnyBaseConverter</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
    </PropertyGroup>

</Project>
